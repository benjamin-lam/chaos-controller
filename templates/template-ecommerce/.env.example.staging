NODE_ENV=staging
BASE_URL=https://staging.example-shop.com
LOGIN_PATH=/login
DASHBOARD_SELECTOR=.dashboard-container
EMAIL_SELECTOR=input[type="email"]
PASSWORD_SELECTOR=input[type="password"]
SUBMIT_SELECTOR=button[type="submit"]

ADMIN_EMAIL=admin@staging.example-shop.com
ADMIN_PASSWORD=<SET-IN-SECRET-STORE>
EDITOR_EMAIL=merchant@staging.example-shop.com
EDITOR_PASSWORD=<SET-IN-SECRET-STORE>
VIEWER_EMAIL=viewer@staging.example-shop.com
VIEWER_PASSWORD=<SET-IN-SECRET-STORE>

ENABLE_SCREENSHOTS=true
ENABLE_TRACES=true
NAVIGATION_TIMEOUT=45000
ACTION_TIMEOUT=15000
